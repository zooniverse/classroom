    "react-copy-to-clipboard": "~5.0.0",
    "react-dom": "~15.6.1",
    "react-redux": "~5.0.6",
    "react-router": "~4.2.0",
    "react-router-dom": "~4.2.2",
    "react-youtube": "~7.9.0",
    "redux": "~3.7.2",
    "redux-devtools-extension": "~2.13.2",
    "redux-logger": "~3.0.6",
    "superagent": "~3.8.3",
