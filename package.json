{
  "name": "edu-api-front-end",
  "version": "1.1.0",
  "description": "Education API front-end application",
  "repository": {
    "type": "git",
    "url": "https://github.com/zooniverse/edu-api-front-end"
  },
  "scripts": {
    "start": "export NODE_ENV=development; BABEL_ENV=development check-engines && check-dependencies && webpack-dashboard -p 3999 -- webpack-dev-server --config ./webpack.config.js",
    "test": "NODE_ENV=development BABEL_ENV=test mocha mocha-setup.js $(find src -name *.spec.jsx) --compilers js:@babel/register --require mock-local-storage || true",
    "eslint": "eslint .",
    "build": "export BABEL_ENV=production; check-engines && check-dependencies && webpack --config webpack.production.config.js -p",
    "deploy-staging": "export NODE_ENV=staging; npm run build && node ./bin/upload.js dist preview.zooniverse.org/classroom",
    "deploy-production": "export NODE_ENV=production; npm run build && node ./bin/upload.js dist classroom.zooniverse.org"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "author": "Zooniverse",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zooniverse/edu-api-front-end"
  },
  "homepage": "https://github.com/zooniverse/edu-api-front-end",
